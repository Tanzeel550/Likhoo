<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane maxHeight="102.0" maxWidth="300.0" minHeight="80.0" minWidth="20.0" prefHeight="80.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.FindDialogController">

    <content>
        <FlowPane alignment="CENTER" maxHeight="200.0" maxWidth="200.0" minHeight="200.0" minWidth="10.0" prefHeight="200.0" prefWidth="200.0">
            <GridPane>
                <Label prefHeight="16.0" prefWidth="58.0" text="Text: " GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                <TextField fx:id="findTextField" GridPane.columnIndex="1" GridPane.rowIndex="0" />
            </GridPane>
            <CheckBox text="Match Case" fx:id="matchCase" alignment="BOTTOM_LEFT"/>
        </FlowPane>
    </content>

</DialogPane>
